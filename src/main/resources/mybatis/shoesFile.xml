<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dev.mvc.shoesFile.ShoesFileDAOInter">
  <select id="list" resultType="dev.mvc.shoesFile.ShoesFileVO">
    SELECT shoes_file_no, name, sizes, ex, src, shoesno
    FROM shoes_file
    ORDER BY shoes_file_no
  </select>

</mapper>
