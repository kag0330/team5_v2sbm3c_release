SELECT 
  uc.table_name,
  uc.constraint_name,
  ucc.column_name,
  uc.r_constraint_name,
  uc.delete_rule
FROM 
  user_constraints uc
  JOIN user_cons_columns ucc ON uc.constraint_name = ucc.constraint_name
WHERE 
  uc.constraint_type = 'R'
ORDER BY 
  uc.table_name;
--TABLE_NAME     CONSTRAINT_NAME/COLUMN_NAME/R_CONSTRAINT_NAME/DELETE_ROLE

--BASKET	         SYS_C008158	MEMBERNO	SYS_C007473	NO ACTION
--BASKET	         SYS_C008159	OPTIONNO	SYS_C007741	NO ACTION
--CATEGORY	         SYS_C007494	PARENTNO	SYS_C007493	NO ACTION
--CATEGORY_LIST	     SYS_C008329	SHOESNO	    SYS_C008114	NO ACTION
--CATEGORY_LIST	     SYS_C008328	CATEGORYNO	SYS_C007493	NO ACTION
--DELIVERY	         SYS_C008361	MEMBERNO	SYS_C007473	NO ACTION
--LOGIN_HISTORY	     SYS_C007484	MEMBERNO	SYS_C007473	NO ACTION
--NOTICE	         SYS_C008269	MEMBERNO	SYS_C007473	NO ACTION
--NOTICE_FILE	     SYS_C008276	NOTICENO	SYS_C008268	NO ACTION
--OTHER_INQUIRY	     SYS_C007816	MEMBERNO	SYS_C007473	NO ACTION
--PAYMENT	         SYS_C008078	MEMBERNO	SYS_C007473	NO ACTION
--PAYMENT_DETAILS	 SYS_C008308	OPTIONNO	SYS_C007741	NO ACTION
--PAYMENT_DETAILS	 SYS_C008309	PAYMENTNO	SYS_C008077	NO ACTION
--PAYMENT_INQUIRY	 SYS_C008086	PAYMENTNO	SYS_C008077	NO ACTION
--PAYMENT_INQUIRY	 SYS_C008085	MEMBERNO	SYS_C007473	NO ACTION
--REPORT	         SYS_C008404	MEMBERNO	SYS_C007473	NO ACTION
--REPORT	         SYS_C008406	TYPENO	    SYS_C008382	NO ACTION
--REPORT	         SYS_C008405	REVIEWNO	SYS_C008143	NO ACTION
--REVIEW	         SYS_C008144	SHOESNO	    SYS_C008114	NO ACTION
--REVIEW	         SYS_C008145	MEMBERNO	SYS_C007473	NO ACTION
--SHOES	             SYS_C008115	MEMBERNO	SYS_C007473	NO ACTION
--SHOES_FILE	     SYS_C008291	SHOESNO	    SYS_C008114	NO ACTION
--SHOES_INQUIRY 	 SYS_C007788	MEMBERNO	SYS_C007473	NO ACTION
--SHOES_INQUIRY	     SYS_C007787	OPTIONNO	SYS_C007741	NO ACTION

-- BASKET 테이블 외래 키 수정
ALTER TABLE BASKET DROP CONSTRAINT SYS_C008158;
ALTER TABLE BASKET ADD CONSTRAINT FK_BASKET_MEMBERNO FOREIGN KEY (MEMBERNO) REFERENCES MEMBER (MEMBERNO) ON DELETE CASCADE;

ALTER TABLE BASKET DROP CONSTRAINT SYS_C008159;
ALTER TABLE BASKET ADD CONSTRAINT FK_BASKET_OPTIONNO FOREIGN KEY (OPTIONNO) REFERENCES OPTIONS (OPTIONNO) ON DELETE CASCADE;

-- CATEGORY 테이블 외래 키 수정
ALTER TABLE CATEGORY DROP CONSTRAINT SYS_C007494;
ALTER TABLE CATEGORY ADD CONSTRAINT FK_CATEGORY_PARENTNO FOREIGN KEY (PARENTNO) REFERENCES CATEGORY (CATEGORYNO) ON DELETE CASCADE;

-- CATEGORY_LIST 테이블 외래 키 수정
ALTER TABLE CATEGORY_LIST DROP CONSTRAINT SYS_C008329;
ALTER TABLE CATEGORY_LIST ADD CONSTRAINT FK_CATEGORY_LIST_SHOESNO FOREIGN KEY (SHOESNO) REFERENCES SHOES (SHOESNO) ON DELETE CASCADE;

ALTER TABLE CATEGORY_LIST DROP CONSTRAINT SYS_C008328;
ALTER TABLE CATEGORY_LIST ADD CONSTRAINT FK_CATEGORY_LIST_CATEGORYNO FOREIGN KEY (CATEGORYNO) REFERENCES CATEGORY (CATEGORYNO) ON DELETE CASCADE;

-- DELIVERY 테이블 외래 키 수정
ALTER TABLE DELIVERY DROP CONSTRAINT SYS_C008361;
ALTER TABLE DELIVERY ADD CONSTRAINT FK_DELIVERY_MEMBERNO FOREIGN KEY (MEMBERNO) REFERENCES MEMBER (MEMBERNO) ON DELETE CASCADE;

-- LOGIN_HISTORY 테이블 외래 키 수정
ALTER TABLE LOGIN_HISTORY DROP CONSTRAINT SYS_C007484;
ALTER TABLE LOGIN_HISTORY ADD CONSTRAINT FK_LOGIN_HISTORY_MEMBERNO FOREIGN KEY (MEMBERNO) REFERENCES MEMBER (MEMBERNO) ON DELETE CASCADE;

-- NOTICE 테이블 외래 키 수정
ALTER TABLE NOTICE DROP CONSTRAINT SYS_C008269;
ALTER TABLE NOTICE ADD CONSTRAINT FK_NOTICE_MEMBERNO FOREIGN KEY (MEMBERNO) REFERENCES MEMBER (MEMBERNO) ON DELETE CASCADE;

-- NOTICE_FILE 테이블 외래 키 수정
ALTER TABLE NOTICE_FILE DROP CONSTRAINT SYS_C008276;
ALTER TABLE NOTICE_FILE ADD CONSTRAINT FK_NOTICE_FILE_NOTICENO FOREIGN KEY (NOTICENO) REFERENCES NOTICE (NOTICENO) ON DELETE CASCADE;

-- OTHER_INQUIRY 테이블 외래 키 수정
ALTER TABLE OTHER_INQUIRY DROP CONSTRAINT SYS_C007816;
ALTER TABLE OTHER_INQUIRY ADD CONSTRAINT FK_OTHER_INQUIRY_MEMBERNO FOREIGN KEY (MEMBERNO) REFERENCES MEMBER (MEMBERNO) ON DELETE CASCADE;

-- PAYMENT 테이블 외래 키 수정
ALTER TABLE PAYMENT DROP CONSTRAINT SYS_C008078;
ALTER TABLE PAYMENT ADD CONSTRAINT FK_PAYMENT_MEMBERNO FOREIGN KEY (MEMBERNO) REFERENCES MEMBER (MEMBERNO) ON DELETE CASCADE;

-- PAYMENT_DETAILS 테이블 외래 키 수정
ALTER TABLE PAYMENT_DETAILS DROP CONSTRAINT SYS_C008308;
ALTER TABLE PAYMENT_DETAILS ADD CONSTRAINT FK_PAYMENT_DETAILS_OPTIONNO FOREIGN KEY (OPTIONNO) REFERENCES OPTIONS (OPTIONNO) ON DELETE CASCADE;

ALTER TABLE PAYMENT_DETAILS DROP CONSTRAINT SYS_C008309;
ALTER TABLE PAYMENT_DETAILS ADD CONSTRAINT FK_PAYMENT_DETAILS_PAYMENTNO FOREIGN KEY (PAYMENTNO) REFERENCES PAYMENT (PAYMENTNO) ON DELETE CASCADE;

-- PAYMENT_INQUIRY 테이블 외래 키 수정
ALTER TABLE PAYMENT_INQUIRY DROP CONSTRAINT SYS_C008086;
ALTER TABLE PAYMENT_INQUIRY ADD CONSTRAINT FK_PAYMENT_INQUIRY_PAYMENTNO FOREIGN KEY (PAYMENTNO) REFERENCES PAYMENT (PAYMENTNO) ON DELETE CASCADE;

ALTER TABLE PAYMENT_INQUIRY DROP CONSTRAINT SYS_C008085;
ALTER TABLE PAYMENT_INQUIRY ADD CONSTRAINT FK_PAYMENT_INQUIRY_MEMBERNO FOREIGN KEY (MEMBERNO) REFERENCES MEMBER (MEMBERNO) ON DELETE CASCADE;

-- REPORT 테이블 외래 키 수정
ALTER TABLE REPORT DROP CONSTRAINT SYS_C008404;
ALTER TABLE REPORT ADD CONSTRAINT FK_REPORT_MEMBERNO FOREIGN KEY (MEMBERNO) REFERENCES MEMBER (MEMBERNO) ON DELETE CASCADE;

ALTER TABLE REPORT DROP CONSTRAINT SYS_C008406;
ALTER TABLE REPORT ADD CONSTRAINT FK_REPORT_TYPENO FOREIGN KEY (TYPENO) REFERENCES REPORT_TYPE (TYPENO) ON DELETE CASCADE;

ALTER TABLE REPORT DROP CONSTRAINT SYS_C008405;
ALTER TABLE REPORT ADD CONSTRAINT FK_REPORT_REVIEWNO FOREIGN KEY (REVIEWNO) REFERENCES REVIEW (REVIEWNO) ON DELETE CASCADE;

-- REVIEW 테이블 외래 키 수정
ALTER TABLE REVIEW DROP CONSTRAINT SYS_C008144;
ALTER TABLE REVIEW ADD CONSTRAINT FK_REVIEW_SHOESNO FOREIGN KEY (SHOESNO) REFERENCES SHOES (SHOESNO) ON DELETE CASCADE;

ALTER TABLE REVIEW DROP CONSTRAINT SYS_C008145;
ALTER TABLE REVIEW ADD CONSTRAINT FK_REVIEW_MEMBERNO FOREIGN KEY (MEMBERNO) REFERENCES MEMBER (MEMBERNO) ON DELETE CASCADE;

-- SHOES 테이블 외래 키 수정
ALTER TABLE SHOES DROP CONSTRAINT SYS_C008115;
ALTER TABLE SHOES ADD CONSTRAINT FK_SHOES_MEMBERNO FOREIGN KEY (MEMBERNO) REFERENCES MEMBER (MEMBERNO) ON DELETE CASCADE;

-- SHOES_FILE 테이블 외래 키 수정
ALTER TABLE SHOES_FILE DROP CONSTRAINT SYS_C008291;
ALTER TABLE SHOES_FILE ADD CONSTRAINT FK_SHOES_FILE_SHOESNO FOREIGN KEY (SHOESNO) REFERENCES SHOES (SHOESNO) ON DELETE CASCADE;

-- SHOES_INQUIRY 테이블 외래 키 수정
ALTER TABLE SHOES_INQUIRY DROP CONSTRAINT SYS_C007788;
ALTER TABLE SHOES_INQUIRY ADD CONSTRAINT FK_SHOES_INQUIRY_MEMBERNO FOREIGN KEY (MEMBERNO) REFERENCES MEMBER (MEMBERNO) ON DELETE CASCADE;

ALTER TABLE SHOES_INQUIRY DROP CONSTRAINT SYS_C007787;
ALTER TABLE SHOES_INQUIRY ADD CONSTRAINT FK_SHOES_INQUIRY_OPTIONNO FOREIGN KEY (OPTIONNO) REFERENCES OPTIONS (OPTIONNO) ON DELETE CASCADE;

